{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "auth-server": "nodemon auth-server/server.js --ignore 'auth-server/*/*.json'",
    "frontend-server": "nodemon frontend-server/server.js",
    "serve": "cd ui-util/ && ng build -w",
    "dev": "start npm run auth-server && start npm run frontend-server && start npm run serve"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "electron": "~1.7.8",
    "express-session": "^1.15.6"
  }
}
